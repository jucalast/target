# Configurações gerais
ENVIRONMENT=development
SECRET_KEY=change-this-to-a-secure-secret-key-in-production

# Configurações do banco de dados
POSTGRES_DB=market_analysis
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@db:5432/market_analysis

# Configurações do PGAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin

# Configurações do NLP
NLP_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2

# Configurações do IBGE/SIDRA
IBGE_API_BASE_URL=https://apisidra.ibge.gov.br/values
CACHE_ENABLED=true
CACHE_TTL_HOURS=24

# Configurações do Google Trends (opcional)
# GOOGLE_TRENDS_API_KEY=your-google-trends-api-key
# GOOGLE_TRENDS_HL=pt-BR
# GOOGLE_TRENDS_TZ=-180

# Configurações de Log
LOG_LEVEL=INFO
LOG_FORMAT=json
